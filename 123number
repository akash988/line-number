#include <stdio.h>

int main() {
    int row,i,j,n=0;
    printf("Enter the row you want\n");
    scanf("%d",&row);
    {
        for(i=1;i<=row;i++)
        
            {
               for(j=1;j<=i;j++)
               
            {
                n++;
                printf("%d",n);
            }
            printf("\n");
        }
    }
    

    return 0;
}
